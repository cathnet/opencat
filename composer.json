{
    "name": "vardot/varbase-project",
    "require": {
        "composer/installers": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "~2.0",
        "drupal/betterlogin": "1.x-dev#f7cf1c5c9ec5e80c6ac0ef04ed605e25495232cd",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/console": "~1.0",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/drupal-library-installer-plugin": "^0.3",
        "drupal/google_analytics_reports": "3.x-dev#2b6bb8efbc7f61ce3c1225638075aa6037b8db44",
        "drupal/group": "^1.0@RC",
        "drupal/h5p": "^1.0@RC",
        "drupal/image_resize_filter": "1.x-dev#c3f4b23b02005859092aaff746b9f21b794adc58",
        "drupal/jquery_colorpicker": "^1.3",
        "drupal/l10n_client": "1.x-dev#9bf8d597732870bdca301512c71b6e5d74d48db2",
        "drupal/legal": "^1.0@beta",
        "drupal/mail_edit": "1.x-dev#bcd0041830d8581b36e6211f0c8eabd8caf9652b",
        "drupal/material_admin": "^1.0-alpha6",
        "drupal/menu_item_role_access": "^1.0",
        "drupal/migrate_manifest": "^1.9",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "^4.3",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/node_edit_protection": "1.x-dev#902339c08222f838030c07aaea23bdc51ababebd",
        "drupal/panels": "^4.4",
        "drupal/pathologic": "^1.0-alpha1",
        "drupal/prepopulate": "2.x-dev#96d7b48dac02d5d126602775f684f89f50412ffd",
        "drupal/quiz": "4.x-dev",
        "drupal/riddler": "^1.1",
        "drupal/search_kint": "^1.0",
        "drupal/security_review": "1.x-dev#35ebae445bb260e961e47c4c58efe7c50c228999",
        "drupal/site_settings": "^1.11",
        "drupal/superfish": "^1.1",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/tour_builder": "1.x-dev#d70e898949b7ec4095efb391a0dbec56d0117558",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^1.9",
        "drupal/twig_xdebug": "^1.1",
        "drupal/video_embed_field": "^2",
        "drupal/views_field_view": "^1.0@beta",
        "drush/drush": "~9.0",
        "fortawesome/font-awesome": "*",
        "jaypan/jquery_colorpicker": "1.0.1",
        "mehrpadin/superfish": "^2.1",
        "oomphinc/composer-installers-extender": "~1.0",
        "vardot/varbase": "~8.7.2",
        "vardot/varbase-updater": "~1.0",
        "webflo/drupal-finder": "~1.0",
        "webmozart/path-util": "~2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "replace": {
        "ajaxorg/ace-builds": "*"
    },
    "type": "project",
    "extra": {
        "_readme": [
            "NOTICE: We're now using composer patches from Vardot repository to suggest",
            "several fixes and better handling of patches in your Drupal project.",
            "You'll notice that we have included (https://github.com/vardot/composer-patches)",
            "in this composer.json repositories. This will replace the original",
            "library (cweagans/composer-patches) with our own from (vardot/composer-patches).",
            "See https://github.com/cweagans/composer-patches/pull/243 and more details",
            "on our changes on the composer-patches package. Once our changes get merged,",
            "we will revert to using (cweagans/composer-patches) without this override."
        ],
        "branch-alias": {
            "dev-8.x-7.x": "8.7.x-dev"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot": [
                "rjzaar/docroot"
            ],
            "docroot/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "docroot/libraries/ace": [
                "npm-asset/ace-builds"
            ],
            "docroot/libraries/masonry": [
                "npm-asset/masonry-layout"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "drupal-libraries": {
            "library-directory": "docroot/libraries",
            "libraries": [
                {
                    "name": "dropzone",
                    "package": "npm-asset/dropzone"
                },
                {
                    "name": "blazy",
                    "package": "npm-asset/blazy"
                },
                {
                    "name": "slick",
                    "package": "npm-asset/slick-carousel"
                },
                {
                    "name": "ace",
                    "package": "npm-asset/ace-builds"
                },
                {
                    "name": "chartjs",
                    "package": "bower-asset/chartjs"
                },
                {
                    "name": "c3",
                    "package": "bower-asset/c3"
                },
                {
                    "name": "d3",
                    "package": "bower-asset/d3"
                },
                {
                    "name": "masonry",
                    "package": "npm-asset/masonry-layout"
                },
                {
                    "name": "imagesloaded",
                    "package": "npm-asset/imagesloaded"
                },
                {
                    "name": "swagger-ui",
                    "package": "swagger-api/swagger-ui"
                }
            ]
        },
        "enable-patching": "1",
        "patches": {
            "drupal/migrate_plus": {
                "Issue #2923824: Nested source for entity_lookup causes an error": "https://www.drupal.org/files/issues/nested-source-for-entity_lookup-causes-an-error-2923824-2.patch"
            },
            "drupal/panels": {
                "Issue #2879001: Warning: array_unique() expects parameter 1 to be array, string given": "https://www.drupal.org/files/issues/panels-ipe-set-classes-arrays-2879001-5.patch"
            },
            "drupal/core": {
                "node-revisions-missing-local-tasks-2885278-11.patch": "https://www.drupal.org/files/issues/2018-04-19/node-revisions-missing-local-tasks-2885278-11.patch",
                "drupal-views-comment-count-2801929-12.patch": "https://www.drupal.org/files/issues/2018-07-10/drupal-views-comment-count-2801929-12.patch",
                "comment-action_exist-2915728-13.patch": "https://www.drupal.org/files/issues/comment-action_exist-2915728-13.patch"
            },
            "drupal/legal": {
                "2988875-4-languageconfig.patch": "https://www.drupal.org/files/issues/2018-08-09/2988875-4-languageconfig.patch"
            },
            "drupal/prepopulate": {
                "prepopulate-entity_reference-2847371-2.patch": "https://www.drupal.org/files/issues/prepopulate-entity_reference-2847371-2.patch"
            },
            "drupal/tour_ui": {
                "tour-ui-2978327-7.patch": "https://www.drupal.org/files/issues/2018-11-03/tour-ui-2978327-7.patch"
            },
            "drupal/pathologic": {
                "3037093-tour-links-are-stripped-out.patch": "https://www.drupal.org/files/issues/2019-03-03/3037093-tour-links-are-stripped-out.patch"
            },
            "drupal/group": {
                "2853001-10.patch": "https://www.drupal.org/files/issues/2018-07-16/2853001-10.patch",
                "3061321-23.patch": "https://www.drupal.org/files/issues/2019-06-21/3061321-23.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "composer-exit-on-patch-failure": false
    },
    "scripts": {
        "drupal-scaffold": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-install-cmd": [
            "Varbase\\composer\\ScriptHandler::createRequiredFiles",
            "Varbase\\composer\\ScriptHandler::removeGitDirectories",
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "Varbase\\composer\\ScriptHandler::createRequiredFiles",
            "Varbase\\composer\\ScriptHandler::removeGitDirectories"
        ],
        "post-drupal-scaffold-cmd": [
            "Varbase\\composer\\ScriptHandler::postDrupalScaffoldProcedure",
            "cd docroot && patch -p1 <../scripts/Add-FollowSymLinks-to-htaccess.patch"
        ]
    },
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Vardot",
            "homepage": "https://github.com/vardot",
            "role": "Maintainer"
        }
    ],
    "description": "Project template for Varbase distribution.",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "jquery": {
            "type": "package",
            "package": {
                "name": "jaypan/jquery_colorpicker",
                "type": "drupal-library",
                "version": "1.0.1",
                "source": {
                    "url": "https://github.com/jaypan/jquery_colorpicker",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "bgrins": {
            "type": "package",
            "package": {
                "name": "bgrins-spectrum",
                "type": "drupal-library",
                "version": "1.8.0",
                "source": {
                    "url": "https://github.com/bgrins/spectrum",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "fontawesome": {
            "type": "package",
            "package": {
                "name": "fortawesome/font-awesome",
                "version": "v4.7.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "fontawesome"
                },
                "dist": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/archive/v4.7.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "composer-patches": {
            "type": "vcs",
            "url": "https://github.com/vardot/composer-patches"
        }
    },
    "support": {
        "issues": "http://drupal.org/project/issues/varbase",
        "source": "http://cgit.drupalcode.org/varbase"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}