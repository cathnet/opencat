<li class="sf-depth-1 menuparent">
    <a  href="{{ url('<front>')['#markup'] }}group/{{ oc_group }}/{% block oc_type %}oc_type{% endblock %}" target="_self" class="is-active sf-depth-1 menuparent ">
        <span class="fa-stack fa-lg ">
  <i class="fa fa-square fa-stack-2x oc_icon_back"></i>
  <i class="fa fa-{% block oc_icon %}oc_icon{% endblock %} fa-stack-1x fa-inverse"></i>
        </span></a>
