<?php

/**
 * @file
 * Contains oc_content.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function occontent_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the oc_content module.
    case 'help.page.oc_content':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Demo content for Opencatechesis') . '</p>';
      return $output;

    default:
  }
}
